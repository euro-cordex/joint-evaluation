    "dask.config.set(\n",
    "    temporary_directory=f\"/mnt/CORDEX_CMIP6_tmp/user_tmp/{os.environ.get('USER')}/dask-tmp\"\n",
    ")\n",
    "# no hyperthreading, netcdf is not threadsafe\n",
    "client = Client(dashboard_address=\"localhost:8888\", threads_per_worker=1)\n",
    "client"
   ]
